---
import Head from "../components/MainHead.astro";

interface Props {
   title?: string;
   description?: string;
}
const { title, description } = Astro.props;
---

<html lang="en" class="bg-neutral">
   <head>
      <Head title={title} description={description} />
   </head>
   <body class="overflow-hidden">
      <slot />
   </body>
</html>
